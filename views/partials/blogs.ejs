<% if(blogs.length > 0){ %>
    <div class="blog-cont">
        <% for(let blog of blogs){ %>
            <a class="link" href="<%= `/blogs/${blog._id}` %>">
                <div class="blog-card">
                    <h2><%= blog.title %></h2>
                    <h4><%= blog.date %></h4>
                </div>
            </a>
        <% } %>
    </div>
<% } %>
